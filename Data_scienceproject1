        "# Outlier removal using Z-score\n",
        "z_scores = zscore(df_iris.select_dtypes(include=['float64', 'int64']))\n",
        "df_iris = df_iris[(abs(z_scores) < 3).all(axis=1)]\n",
        "z_scores"
